.modal#valuation-urls-creation-modal
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{data: { dismiss: "modal" }} &times;
        %h4.modal-title Add a URL
      .modal-body
        = render "valuation_urls/valuation_url_form", valuation_url: @valuation_url, appraisal: @appraisal, organization: @organization
